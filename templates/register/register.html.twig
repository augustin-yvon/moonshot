{% extends 'base.html.twig' %}

{% block body %}
    <form method="post">
        {{ form_start(form) }}

        {{ form_row(form.firstname) }}
        {{ form_row(form.lastname) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}

        <button type="submit" class="btn btn-primary">S'inscrire</button>

        {{ form_end(form) }}
    </form>
{% endblock %}